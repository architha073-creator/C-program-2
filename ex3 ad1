#include <stdio.h>
 #include <conio.h>
 int main()
 {
	int i,sum=0,n,num;
	clrscr();
	printf("Enter a no:");
	scanf("%d",&n);
	num=n;
	while(n!=0)
	{
		 i=n%10;
		 sum=sum*10+i;
		 n=n/10;
	}
	if (sum==num)
	{
		printf("palindrome:");
	}
	else
	{
		printf("not a palindrom :");
	}
	getch();
	return 0;
 }
