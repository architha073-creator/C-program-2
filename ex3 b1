#include<stdio.h>
#include<conio.h>
int main()
{
	int num;
	clrscr();
	printf("Enter element to check:");
	scanf("%d",&num);
	if (num==0)
	{
		printf("you have entered Zero");
	}
	else if(num%2==0)
	{
		printf("The num %d is even",num);
	}
	else
	{
		printf("The num %d is odd",num);
	}
	getch();
	return 0;
	}
