#include<stdio.h>
#include<conio.h>
int main()
{
	int units,r,price;
	clrscr();
	printf("Enter units consumed:");
	scanf("%d",&units);
	if (units<100)
	{
	     price=units*5;
	}
	else if (units<=300)
	{
		price=100*5+(units-100)*7;
	}
	else
	{
		price=100*5+200*7+(units-300)*10;
	}
	printf("Your Electricity Bill:%d",price);
	getch();
	return 0;
	}
