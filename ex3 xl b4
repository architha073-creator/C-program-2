#include<stdio.h>
#include <conio.h>
int main()
{
	int i=1,n,s_odd=0,s_even=0,sum=0;
	clrscr();
	printf("Enter natural no:");
	scanf("%d",&n);
	while(i<=n)
	{
	  sum=sum+i;

	  if (i%2==0)
	  {
		s_even+=i;
	  }
	  else
	  {
		s_odd+=i;
	  }
	  i=i+1;
	  }
	  printf("sum=%d\n",sum);
	  printf("s_even=%d\n",s_even);
	  printf("s_odd=%d\n",s_odd);
	  getch();
	  return 0;
	  }
